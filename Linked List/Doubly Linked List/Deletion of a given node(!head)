
public class Solution {
    public static Node deleteNode(Node head, int k) {
         int cnt = 1;

        Node temp = head;


        if(k == 1){
            head = head.next;
            return head;
        }

        while(temp.next != null){
            if(cnt == k){
                break;
            }
             cnt++;
            temp = temp.next;
        }
        temp.prev.next = temp.next;

        if(temp.next != null){
            temp.next.prev = temp.prev;
        }
        return head;
    }
}
