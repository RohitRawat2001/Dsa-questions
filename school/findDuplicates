
class Solution {
    public List<Integer> findDuplicates(int[] arr) {
        
    List<Integer> l1 = new ArrayList<>();
        
        Arrays.sort(arr);
        
        for(int i = 0; i<arr.length-1;i++){
            if(arr[i]==arr[i+1]){
                l1.add(arr[i]);
            }
        }
        return l1;
    }
}
